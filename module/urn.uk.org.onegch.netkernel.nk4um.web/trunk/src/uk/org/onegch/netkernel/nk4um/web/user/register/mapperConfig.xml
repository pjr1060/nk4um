<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xrl="http://netkernel.org/xrl">
  <endpoint>
    <id>nk4um:web:user:register</id>
    <grammar>res:/nk4um/user/register</grammar>
    <header name="nk4umAutoStyle">true</header>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.user.register.RegisterFormAccessor</argument>
    </request>
  </endpoint>
  <endpoint>
    <id>nk4um:web:user:register:doRegister</id>
    <grammar>
      res:/nk4um/user/doRegister
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.user.register.DoRegisterAccessor</argument>
    </request>
  </endpoint>
  <endpoint>
    <id>nk4um:web:url:user:register</id>
    <grammar>nk4um:web:url:user:register</grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.user.register.RegisterUrlAccessor</argument>
    </request>
  </endpoint>
</config>