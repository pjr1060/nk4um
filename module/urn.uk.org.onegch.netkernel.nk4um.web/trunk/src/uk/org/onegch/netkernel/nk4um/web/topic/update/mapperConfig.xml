<?xml version="1.0" encoding="UTF-8"?>
<config>
  <endpoint>
    <id>nk4um:web:topic:doUpdate</id>
    <grammar>
      res:/nk4um/forum/topic/
      <group name="id"><regex type="integer"/></group>
      /doUpdate
    </grammar>
    <role>User</role>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.topic.update.DoUpdateAccessor</argument>
      <argument name="id" method="as-string">arg:id</argument>
    </request>
  </endpoint>
</config>