<?xml version="1.0" encoding="UTF-8"?>
<nk4um:page xmlns:nk4um="http://onegch.org.uk/netkernel/nk4um"
            xmlns:xrl="http://netkernel.org/xrl">
  <nk4um:title>
    <nk4um:group>
      <xrl:eval>
        <xrl:xpath>..</xrl:xpath>
        <xrl:identifier>nk4um:web:forum:title</xrl:identifier>
        <xrl:argument name="id">arg:id</xrl:argument>
      </xrl:eval>
    </nk4um:group>
    - nk4um
  </nk4um:title>
  <nk4um:heading>nk4um Forum</nk4um:heading>
  <nk4um:tagLine>
      <xrl:eval>
        <xrl:xpath>..</xrl:xpath>
        <xrl:identifier>nk4um:web:forum:description</xrl:identifier>
        <xrl:argument name="id">arg:id</xrl:argument>
      </xrl:eval>
  </nk4um:tagLine>
  <nk4um:head/>
  <nk4um:breadcrumbs>
    <xrl:include>
      <xrl:identifier>nk4um:web:forum:breadcrumbs</xrl:identifier>
      <xrl:argument name="id">arg:id</xrl:argument>
    </xrl:include>
  </nk4um:breadcrumbs>
  <nk4um:content>
    <xrl:include>
      <xrl:identifier>nk4um:web:forum</xrl:identifier>
      <xrl:argument name="id">arg:id</xrl:argument>
    </xrl:include>
  </nk4um:content>
</nk4um:page>
