<?xml version="1.0" encoding="UTF-8"?>
<config>
  <endpoint>
    <grammar>
      <active>
        <identifier>nk4um:web:forum:topicList</identifier>
        <argument name="id"/>
        <argument name="moderator"/>
      </active>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.forum.list.TopicListAccessor</argument>
      <argument name="id">arg:id</argument>
      <argument name="moderator">arg:moderator</argument>
    </request>
  </endpoint>
  <endpoint>
    <grammar>
      <active>
        <identifier>nk4um:web:forum:topic</identifier>
        <argument name="id"/>
      </active>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.forum.list.TopicAccessor</argument>
      <argument name="id">arg:id</argument>
    </request>
  </endpoint>
  <endpoint>
    <grammar>
      <active>
        <identifier>nk4um:web:forum:topicLastPost</identifier>
        <argument name="topicId"/>
      </active>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.forum.list.TopicLastPostAccessor</argument>
      <argument name="topicId">arg:topicId</argument>
    </request>
  </endpoint>
</config>