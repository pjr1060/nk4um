<?xml version="1.0" encoding="UTF-8"?>
<config>
  <endpoint>
    <grammar>res:/nk4um/forums/</grammar>
    <header name="WrappedControlPanel"><literal type="boolean">true</literal></header>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.admin.forums.list.ListAccessor</argument>
    </request>
  </endpoint>
  <endpoint>
    <grammar>
      <group>
        res:/nk4um/forum/
        <group name="id"><regex type="integer"/></group>
        /
      </group>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.admin.user.details.DetailsAccessor</argument>
      <argument name="id" method="as-string">arg:id</argument>
    </request>
  </endpoint>
  <endpoint>
    <grammar>
      <active>
        <identifier>nk4um:admin:forums:forumGroup</identifier>
        <argument name="id"/>
      </active>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.admin.forums.list.ForumGroupAccessor</argument>
      <argument name="id">arg:id</argument>
    </request>
  </endpoint>
  <endpoint>
    <grammar>
      <active>
        <identifier>nk4um:admin:forums:forumList</identifier>
        <argument name="id"/>
      </active>
    </grammar>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.admin.forums.list.ForumListAccessor</argument>
      <argument name="id">arg:id</argument>
    </request>
  </endpoint>
</config>