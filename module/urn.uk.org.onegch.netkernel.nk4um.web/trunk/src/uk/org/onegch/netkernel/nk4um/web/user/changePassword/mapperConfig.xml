<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xrl="http://netkernel.org/xrl">
  <endpoint>
    <id>nk4um:web:user:changePassword</id>
    <grammar>res:/nk4um/user/changePassword</grammar>
    <header name="nk4umAutoStyle">true</header>
    <role>User</role>
    <request>
      <identifier>res:/uk/org/onegch/netkernel/nk4um/web/user/changePassword/changePassword.xml</identifier>
    </request>
  </endpoint>
  <endpoint>
    <id>nk4um:web:user:doChangePassword</id>
    <grammar>
      res:/nk4um/user/doChangePassword
    </grammar>
    <role>User</role>
    <request>
      <identifier>active:java</identifier>
      <argument name="class">uk.org.onegch.netkernel.nk4um.web.user.changePassword.DoChangePasswordAccessor</argument>
    </request>
  </endpoint>
</config>
