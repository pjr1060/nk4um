<config>
	
	<endpoint>
		<grammar>
			<active>
				<identifier>active:indexNK4UM</identifier>
				<argument name="id"/>
			</active>
		</grammar>
		<request>
			<identifier>active:groovy</identifier>
			<argument name="operator">res:/org/netkernelroc/nk4um/web/search/index.gy</argument>
			<argument name="id">arg:id</argument>
		</request>
	</endpoint>
	
	<endpoint>
		<grammar>
			<active>
				<identifier>active:indexResetNK4UM</identifier>
			</active>
		</grammar>
		<request>
			<identifier>active:groovy</identifier>
			<argument name="operator">res:/org/netkernelroc/nk4um/web/search/indexReset.gy</argument>
		</request>
	</endpoint>
	
	<endpoint>
		<grammar>
			<active>
				<identifier>active:searchNK4UM</identifier>
				<argument name="search"/>
			</active>
		</grammar>
		<request>
			<identifier>active:groovy</identifier>
			<argument name="operator">res:/org/netkernelroc/nk4um/web/search/search.gy</argument>
			<argument name="search">arg:search</argument>
		</request>
	</endpoint>
	
	<endpoint>
		<grammar>active:searchConfig</grammar>
		<request>
			<identifier>active:groovy</identifier>
			<argument name="operator">res:/org/netkernelroc/nk4um/web/search/searchConfig.gy</argument>
		</request>
	</endpoint>
	
</config>